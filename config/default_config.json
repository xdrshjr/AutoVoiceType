{
    "api": {
        "dashscope_api_key": "",
        "base_websocket_url": "wss://dashscope.aliyuncs.com/api-ws/v1/inference",
        "model": "fun-asr-realtime"
    },
    "audio": {
        "sample_rate": 16000,
        "channels": 1,
        "chunk_size": 3200,
        "format": "pcm"
    },
    "hotkey": {
        "trigger_key": "ctrl_r",
        "description": "Right Control Key"
    },
    "input": {
        "preferred_method": "clipboard",
        "input_delay": 0.05,
        "paste_delay": 0.1,
        "restore_clipboard": true,
        "max_input_length": 10000
    },
    "general": {
        "auto_start": false,
        "language": "zh-CN",
        "log_level": "INFO"
    },
    "recognition": {
        "semantic_punctuation_enabled": false,
        "timeout": 30
    }
}

